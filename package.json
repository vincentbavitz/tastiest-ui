{
  "name": "@tastiest-io/tastiest-ui",
  "author": "vincentbavitz",
  "description": "Tastiest's complete UI library.",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "git://github.com:tastiest-io/tastiest-ui",
  "version": "0.45.0",
  "main": "dist/index.js",
  "module": "dist/tastiest-ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "rollup watch",
    "build": "TAILWIND_MODE=build NODE_ENV=production yarn rimraf ./dist/** && yarn lint && yarn rollup -c",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet --fix",
    "prepare": "install-peers && rollup -c",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/tastiest-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@ant-design/icons": "^4.7.0",
    "clsx": "^1.1.1",
    "next": ">=11",
    "react": ">=16",
    "react-use": "^17.3.1",
    "styled-components": "^5.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@nivo/core": "^0.74.0",
    "@nivo/line": "^0.74.0",
    "@popperjs/core": "^2.10.2",
    "@tastiest-io/tastiest-icons": "^0.28.0",
    "react-popper": "^2.2.5",
    "react-table": "^7.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.15.8",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@size-limit/preset-small-lib": "^6.0.3",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-table": "^7.7.8",
    "@types/styled-components": "^5.1.15",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "install-peers-cli": "^2.2.0",
    "less": "^4.1.2",
    "next": "^12.0.3",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-partial-import": "^4.1.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.0",
    "react": ">=16",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-is": "^17.0.2",
    "react-use": "^17.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rsuite": "^5.2.3",
    "size-limit": "^6.0.3",
    "styled-components": "^5.3.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
